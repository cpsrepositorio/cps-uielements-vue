{
  "name": "@cps/uielements-vue",
  "description": "Biblioteca de UI em conformidade com o CPS Design System, constru√≠da com Vue 3, Vite e Tailwind.",
  "author": "Erick Eduardo Petrucelli <erick.petrucelli@cps.sp.gov.br> (https://github.com/ErickPetru)",
  "version": "0.1.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cpsrepositorio/cps-uielements-vue.git"
  },
  "homepage": "https://cpsrepositorio.github.io/cps-uielements-vue/",
  "engines": {
    "node": ">=14"
  },
  "type": "module",
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/cps-uielements-vue.umd.cjs",
  "module": "./dist/cps-uielements-vue.es.js",
  "types": "./dist/cps-uielements-vue.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cps-uielements-vue.umd.cjs",
      "import": "./dist/cps-uielements-vue.es.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite --port 6005",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "storybook:deploy": "npm run storybook:build && gh-pages -d storybook-static",
    "build": "run-p type-check build:vue",
    "build:vue": "vite build",
    "build:types": "vue-tsc --emitDeclarationOnly --declaration",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:fix": "npm run lint --fix",
    "prettier:fix": "prettier --write .",
    "postinstall": "rm -rf node_modules/@types/react",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "tailwindcss": "^3.1.0",
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@rushstack/eslint-patch": "^1.1.4",
    "@storybook/addon-a11y": "^6.5.9",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-vite": "^0.2.2",
    "@storybook/vue3": "^6.5.9",
    "@types/node": "^18.6.1",
    "@types/webfontloader": "^1.6.34",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "eslint": "^8.20.0",
    "eslint-plugin-storybook": "^0.6.1",
    "eslint-plugin-vue": "^9.3.0",
    "gh-pages": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "storybook-dark-mode": "^1.1.0",
    "tailwindcss": "^3.1.6",
    "typescript": "^4.7.4",
    "vite": "^3.0.3",
    "vite-plugin-dts": "^1.4.1",
    "vite-svg-loader": "^3.4.0",
    "vue": "^3.2.37",
    "vue-loader": "^17.0.0",
    "vue-tsc": "^0.39.5"
  }
}
